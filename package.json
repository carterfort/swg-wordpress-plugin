    "storybook-chrome-screenshot": "^1.4.0",
    "stylelint": "^9.10.1",
    "stylelint-config-wordpress": "^13.1.0",
    "stylelint-declaration-use-variable": "^1.7.0",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.13.0",
    "svgo": "^1.3.0",
